<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="org.kde.KScreen">
        <signal name="outputConnected">
            <arg type="s" name="outputName" direction="out" />
        </signal>
        <signal name="unknownOutputConnected">
            <arg type="s" name="outputName" direction="out" />
        </signal>
        <method name="listCurrentProfiles">
            <arg type="a{ss}" name="profiles" direction="out" />
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="StringMap"/>
        </method>
        <method name="activeProfile">
            <arg type="s" name="id" direction="out" />
        </method>
        <method name="activateProfile">
            <arg type="s" name="id" direction="in" />
        </method>
        <method name="createProfileFromCurrentConfig">
            <arg type="s" name="name" direction="in" />
            <arg type="b" name="preferred" direction="in" />
            <arg type="s" name="id" direction="out" />
        </method>
        <method name="deleteProfile">
            <arg type="s" name="id" direction="in" />
        </method>
        <method name="getProfile">
            <arg type="s" name="id" direction="in" />
            <arg type="a{sv}" name="profile" direction="out" />
            <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="QVariantMap"/>
        <signal name="profilesChanged"/>
    </interface>
</node>
